<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.ShouyinyuanDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.ShouyinyuanEntity" id="shouyinyuanMap">
        <result property="shouyinyuangonghao" column="shouyinyuangonghao"/>
        <result property="mima" column="mima"/>
        <result property="shouyinyuanxingming" column="shouyinyuanxingming"/>
        <result property="xingming" column="xingming"/>
        <result property="nianling" column="nianling"/>
        <result property="zhaopian" column="zhaopian"/>
        <result property="shouyinyuanshouji" column="shouyinyuanshouji"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.ShouyinyuanVO" >
		SELECT * FROM shouyinyuan  shouyinyuan         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.ShouyinyuanVO" >
		SELECT  shouyinyuan.* FROM shouyinyuan  shouyinyuan 	
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.ShouyinyuanView" >

		SELECT  shouyinyuan.* FROM shouyinyuan  shouyinyuan 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.ShouyinyuanView" >
		SELECT * FROM shouyinyuan  shouyinyuan <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

</mapper>
